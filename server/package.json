{
  "name": "nxvms-server",
  "version": "0.1.4",
  "description": "NXvms Server - Video Management System Backend",
  "author": "NXvms Team",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "typeorm migration:run -d src/database/data-source.ts",
    "db:revert": "typeorm migration:revert -d src/database/data-source.ts",
    "db:seed": "ts-node -P tsconfig.json src/database/seeders/seed.ts",
    "script:add-camera": "ts-node -P tsconfig.json src/scripts/add-camera.ts",
    "script:health-check": "ts-node -P tsconfig.json src/scripts/health-check.ts",
    "script:verify-system": "ts-node -P tsconfig.json src/scripts/verify-system.ts",
    "script:pre-testing": "node src/scripts/pre-testing.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "openapi:generate": "nest-swagger"
  },
  "dependencies": {
    "@fastify/static": "^6.12.0",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/platform-fastify": "^10.2.10",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "chokidar": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "fluent-ffmpeg": "^2.1.2",
    "mqtt": "^5.14.1",
    "node-disk-info": "^1.3.0",
    "node-rsa": "^1.1.1",
    "onvif": "^0.6.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.16",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.10",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.99",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.16",
    "typescript": "^5.3.3"
  }
}